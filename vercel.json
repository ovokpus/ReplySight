{
    "functions": {
        "api/api.py": {
            "runtime": "python3.9"
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "api/api.py"
        },
        {
            "handle": "filesystem"
        }
    ],
    "env": {
        "LANGCHAIN_TRACING_V2": "true",
        "LANGCHAIN_ENDPOINT": "https://api.smith.langchain.com"
    }
}