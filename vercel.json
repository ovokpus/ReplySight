{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/respond",
      "destination": "/api/respond.py"
    },
    {
      "source": "/api/health",
      "destination": "/api/health.py"
    },
    {
      "source": "/api/workflow/graph",
      "destination": "/api/workflow/graph.py"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/frontend/$1"
    }
  ],
  "functions": {
    "api/respond.py": {},
    "api/health.py": {},
    "api/workflow/graph.py": {}
  },
  "env": {
    "API_BASE_URL": "https://replysight.vercel.app",
    "NODE_ENV": "production"
  },
  "regions": ["iad1"],
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "cd frontend && npm install"
}