{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/respond",
      "destination": "/api/respond.py"
    },
    {
      "source": "/api/health",
      "destination": "/api/health.py"
    },
    {
      "source": "/api/workflow/graph",
      "destination": "/api/workflow/graph.py"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/frontend/$1"
    }
  ],

  "env": {
    "API_BASE_URL": "https://replysight.vercel.app",
    "NODE_ENV": "production"
  },
  "regions": ["iad1"]
}